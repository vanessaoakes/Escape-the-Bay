   "source": [
    "# here's all the code so far in one place, it's only 11 lines\n",
    "# this is all valuable because it demonstrates our group has the capability\n",
    "# to parse and by extension visualize ANY census data in existence using exclusively our own code.\n",
    "\n",
    "'''\n",
    "import pandas as pd\n",
    "\n",
    "file = \"BP_2016_00A3_with_ann.csv\" #load and read csv\n",
    "df_original = pd.read_csv(file)\n",
    "df_original[\"EMPSZES.id\"] = pd.to_numeric(df_original[\"EMPSZES.id\"]) #convert a column to float\n",
    "all_businesses_df = df_original.loc[df_original[\"EMPSZES.id\"] == 1, :] #filter by industry size\n",
    "df_groupedby_GEOID = df_original.groupby('NAICS.id') #group by industry type\n",
    "df_quick_sum = df_groupedby_GEOID[\"ESTAB\"].sum() #take the sum of the group\n",
    "all_sizes_df = all_businesses_df.loc[all_businesses_df[\"NAICS.id\"] == \"0\", :] #filter by industry type\n",
    "all_sizes_df.head(5) #result is a dataframe only containing only one county code per \"total # of establishments\"\n",
    "'''"
   ]
